function R = corr_clustering_complete(Advice)

n = size(Advice,1);

[MB, partition] = to_block_form(Advice);

Advice = Advice + Advice' + eye(n);
cost = zeros(1,n);

for i=1:n
    v = Advice(:,i);
    cost(i) = v' * MB * v;
end

[min_cost, idx] = min(cost);

R = full(Advice(:,idx(1)));